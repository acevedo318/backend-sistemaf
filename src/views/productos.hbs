<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nombre</th>
            <th scope="col">Precio</th>
            <th scope="col">Costo</th>
        </tr>
    </thead>
    <tbody>

        {{#each productos}}
        <tr>
            <th scope="row">{{this.id}}</th>
            <td>{{this.nombre}}</td>
            <td>{{this.precio}}</td>
            <td>{{this.costo}}</td>


        </tr>
        {{/each}}
    </tbody>
</table>